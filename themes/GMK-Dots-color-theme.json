{
	"name": "GMK Dots",
	"type": "dark",
	"colors": {
		// Dark Blue:  							#191b25
		// Dark Blue light: 				#262938
		// Dark Blue light lighter: #323649
		// Red:        							#fe494e
		// Purple:     							#9b57ff
		// Blue:       							#36c1f6
		// Green:      							#32cb8b
		// Yellow:     							#ffd751
		// Orange:     							#ff9552
		// White:			 							#fafafa
		// Grey:			 							#7e89AA

		"foreground": "#fafafa",
		"selection.background": "#36c1f6",

		"textLink.foreground": "#36c1f6",

		"button.background": "#36c1f6",
		"button.foreground": "#fafafa",

		"dropdown.background": "#262938",
